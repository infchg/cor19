        <!--script src="https://www.chartjs.org/dist/2.9.3/Chart.js"></script--> <!--after n not min-->
        <script src="https://www.chartjs.org/dist/2.7.2/Chart.js"></script> 

  <script type="text/javascript" src="/assets/application.js"></script>
